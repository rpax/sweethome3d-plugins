<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane cacheShape="false" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.massisframework.sweethome3d.plugins.components.fx.ComponentInfoController">
	<children>
		<ScrollPane fx:id="componentsScrollPane" fitToHeight="true"
			fitToWidth="true" hbarPolicy="NEVER" vbarPolicy="AS_NEEDED"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<content>
				<AnchorPane>
					<children>
						<VBox fx:id="componentsAccordion" />
					</children>
				</AnchorPane>
			</content>
		</ScrollPane>
	</children>
</AnchorPane>
